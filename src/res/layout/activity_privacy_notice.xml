<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res-auto" setting:id="@id/agree_id" setting:background="@color/common_color_white" setting:layout_width="match_parent" setting:layout_height="match_parent">
