        <LinearLayout setting:orientation="vertical" setting:layout_width="match_parent" setting:layout_height="match_parent">
            <TextView setting:textSize="12dp" setting:textColor="#d9000000" setting:id="@id/useragreement_content_text1" setting:paddingLeft="16dp" setting:paddingRight="16dp" setting:layout_width="match_parent" setting:layout_height="wrap_content" setting:lineSpacingExtra="5dp" />
            <TextView setting:textSize="12dp" setting:textColor="#d9000000" setting:id="@id/useragreement_content_text2" setting:paddingLeft="16dp" setting:paddingRight="16dp" setting:layout_width="match_parent" setting:layout_height="wrap_content" setting:lineSpacingExtra="5dp" />
            <TextView setting:textSize="12dp" setting:textColor="#d9000000" setting:id="@id/useragreement_content_text3" setting:paddingLeft="16dp" setting:paddingRight="16dp" setting:layout_width="match_parent" setting:layout_height="wrap_content" setting:lineSpacingExtra="5dp" />
