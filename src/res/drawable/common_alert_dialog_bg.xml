<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:drawable="@color/common_black_5alpha" />
    <item android:state_pressed="true" android:drawable="@color/common_black_5alpha" />
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_normal_titlebar_title" />
            <corners android:radius="3dp" />
            <stroke android:width="1dp" android:color="@color/color_normal_titlebar_title" />
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp" />
        </shape>
    </item>
</selector>
